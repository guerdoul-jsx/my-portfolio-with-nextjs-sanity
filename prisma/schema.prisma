generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
  relationMode = "prisma"
}

model Data {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  ip        String
  status    Boolean
  useragent String
  geoip1    String
  geoip2    String
  isp       String
  org       String
  asn       String
}

